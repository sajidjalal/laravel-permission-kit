{
  "name": "sajidjalal/laravel-permission-kit",
  "description": "Role & permission management package extracted from InsureTech CRM",
  "type": "library",
  "license": "proprietary",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "illuminate/database": "^10.0|^11.0|^12.0",
    "illuminate/routing": "^10.0|^11.0|^12.0",
    "illuminate/view": "^10.0|^11.0|^12.0",
    "illuminate/http": "^10.0|^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "InsureTech\\RolePermission\\": "src/"
    },
    "files": [
      "src/Helpers/role_permission_helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "InsureTech\\RolePermission\\RolePermissionServiceProvider"
      ]
    }
  }
}
